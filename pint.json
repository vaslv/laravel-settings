{
    "preset": "laravel",
    "rules": {
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "case",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "method_public",
                "method_protected",
                "method_private",
                "method_public_static",
                "method_protected_static",
                "method_private_static"
            ],
            "sort_algorithm": "alpha"
        }
    }
}
